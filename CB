function Chat(msg)
    if game.PlaceId == 142823291 then -- MM2 SUPPORT
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg, "normalchat")
    elseif game:GetService("TextChatService").ChatVersion == Enum.ChatVersion.TextChatService then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(msg)
    elseif game.PlaceId == 11834272744 then
        game.ReplicatedStorage.newsbroadcast:FireServer(msg, "discord.gg/")
        game.ReplicatedStorage.DefaultChatSystemChatEvents("SayMessageRequest"):FireServer(msg, "all")
    elseif game.PlaceId == 286090429 then
        game.Players.LocalPlayer:Kick("Using RBC with arsenal will get you banned. Wait for a patch.")
    elseif game.PlaceId == 237590761 or game.PlaceId == 237590657 or game.PlaceId == 290815963 then -- Apocalypse rising reborn
        workspace.Remote.Chat:FireServer("Global", msg)
    elseif game.PlaceId == 863266079 then
        game.ReplicatedStorage["Networking\13"]["RemoteEvent\13"]:FireServer("Player Chatted", "Global", msg)
    else
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg, "all")
    end
end

