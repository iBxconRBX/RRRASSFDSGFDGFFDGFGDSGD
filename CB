-- Check if a RBC developer is in game already, and check from now on.

for _, player in ipairs(PlayerChildren) do
    if table.find(owner_usernames, player.Name) and game:GetService("Players").LocalPlayer.Name ~= player.Name then
        Rayfield:Notify({
            Title = "RBC Info",
            Content = "An owner of RBC, " .. player.Name .. ", is in your server. Go up and say hi!",
            Duration = 6.5,
            Image = 4578566027,
            Actions = { -- Notification Buttons
               Ignore = {
                  Name = "Okay!",
                  Callback = function()
               end
            },
         },
         })
    end
end

