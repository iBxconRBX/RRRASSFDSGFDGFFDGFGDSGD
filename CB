if game.PlaceId == 11834272744 then
    local Section = Tab:CreateSection("Nations Roleplay 2 Trollage")
 
    local Button = Tab:CreateButton({
         Name = "Free Paint Map",
         Callback = function()
            local provinces = workspace.Map:GetDescendants()
            local Player = game:GetService("Players").LocalPlayer
            local color = Color3.fromRGB(255, 255, 255)
            
            if not Player.Character:FindFirstChild("PaintBucket") then
                Player.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("PaintBucket"))
            end
            
            for _, province in ipairs(provinces) do
                if province.Name == "Province" then
                    Player.Character.HumanoidRootPart.CFrame = province.CFrame
                    
                    Player.Character.PaintBucket.Remotes.ServerControls:InvokeServer("PaintPart", {["Part"] = province, ["Color"] = color})        
                    
                end
            end
        end,
    })
end
